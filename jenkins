server {
    server_name jenkins.pingamarina.site;
    client_max_body_size 50M;

    location / {
	proxy_pass http://localhost:8000/;
        include proxy_params;
	proxy_buffering off;
	proxy_request_buffering off;
     }

     access_log /var/log/nginx/jk_access.log;
     error_log /var/log/nginx/jk_error.log;
}
